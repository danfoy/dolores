{
  "name": "@danfoy/dolores",
  "type": "module",
  "version": "0.0.3",
  "description": "Discord module for Shodan",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "test:coverage": "c8 npm run test",
    "stage": "nodemon index.js stage",
    "release": "standard-version",
    "publish": "git push origin --atomic --follow-tags",
    "deploy:commands": "node ./utils/deployCommands.js"
  },
  "author": "Dan Foy",
  "license": "MIT",
  "repository": "https://github.com/danfoy/dolores.git",
  "devDependencies": {
    "c8": "^7.12.0",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "sinon": "^15.0.1",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "discord-api-types": "^0.26.1",
    "discord.js": "^14",
    "dotenv": "^16.0.3"
  },
  "c8": {
    "all": true,
    "reporter": [
      "text"
    ],
    "exclude": [
      "data",
      "test"
    ]
  },
  "standard-version": {
    "header": "# Changelog\n\nThis file is automatically generated and updated adhering to [SemVer](https://semver.org) conventions via [semantic-release](https://github.com/conventional-changelog/standard-version), as per the [Conventional Commits](https://www.conventionalcommits.org/en/v1.0.0/) project.\n\n\n",
    "releaseCommitMessageFormat": "build(release): bump package to {{currentTag}}",
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "build",
        "section": "Backend"
      },
      {
        "type": "docs",
        "section": "Backend"
      },
      {
        "type": "style",
        "section": "Backend"
      },
      {
        "type": "refactor",
        "section": "Backend"
      },
      {
        "type": "test",
        "section": "Backend"
      }
    ]
  }
}
